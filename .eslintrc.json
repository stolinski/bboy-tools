{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true
  },
  "settings": {
    "ecmascript": 6,
    "jsx": true,
    "import/core-modules": [
      "meteor/server-render",
      "meteor/tracker",
      "meteor/server-render",
      "meteor/dburles:factory",
      "meteor/gadicohen:sitemaps",
      "meteor/tunifight:loggedin-mixin",
      "meteor/webapp",
      "meteor/levelup:react-accounts-ui",
      "meteor/practicalmeteor:chai",
      "meteor/aldeed:collection2",
      "meteor/mdg:validated-method",
      "meteor/std:accounts-ui",
      "meteor/okgrow:accounts-ui-react",
      "meteor/check",
      "meteor/mongo",
      "meteor/meteorhacks:ssr",
      "meteor/meteor",
      "meteor/email",
      "meteor/react-meteor-data",
      "meteor/jquery",
      "meteor/accounts-base",
      "meteor/random",
      "meteor/alanning:roles",
      "meteor/orionsoft:react-meteor-data",
      "meteor/percolate:synced-cron"
    ],
    "import/resolver": {
      "node": {
        "paths": ["."]
      }
    }
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "experimentalDecorators": true,
      "jsx": true
    },
    "sourceType": "module"
  },
  "extends": ["airbnb", "prettier"],
  "plugins": ["react", "prettier", "graphql", "import"],
  "rules": {
    "react/prefer-stateless-function": 0,
    "prettier/prettier": "error",
    "spaced-comment": 0,
    "quotes": 0,
    "no-shadow": 0,
    "import/first": 0,
    "react/no-typos": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "object-curly-newline": 0,
    "jsx-a11y/no-noninteractive-element-interactions": 0,
    "jsx-a11y/label-has-for": 0,
    "no-console": "off",
    "react/no-multi-comp": 0,
    "import/prefer-default-export": 0,
    "import/no-absolute-path": 0,
    "jsx-a11y/href-no-hash": 0,
    "radix": 0,
    "generator-star-spacing": 0,
    "no-bitwise": ["error", { "allow": ["~"] }],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/forbid-prop-types": [0],
    "no-underscore-dangle": [2, { "allow": ["_id"] }],
    "no-use-before-define": [0],
    "max-len": ["error", 160],
    "import/extensions": [1, "never"],
    "jsx-a11y/no-static-element-interactions": [0],
    "no-return-assign": [0],
    "func-names": [0],
    "indent": ["error", 2],
    "linebreak-style": ["error", "unix"],
    "semi": ["error", "always"]
  },
  "globals": {
    "cy": true,
    "paypal": true,
    "StripeCheckout": true,
    "Stripe": true,
    "before": true,
    "describe": true,
    "it": true,
    "Meteor": true,
    "test": true,
    "expect": true,
    "Session": true,
    "Assets": true,
    "LoggedInMixin": true
  }
}
